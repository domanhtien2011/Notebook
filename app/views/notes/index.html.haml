- if user_signed_in?
  = link_to("Add A New Note", new_note_path)
  %p= link_to("Sign Out", destroy_user_session_path)
- else
  %p= link_to("Sign In", new_user_session_path)
  %p= link_to("Sign Up", new_user_registration_path)

- @notes.each do |note|
  %h2= link_to note.title, note
  %p= time_ago_in_words(note.created_at)

%p= link_to("Home Page", root_path)



